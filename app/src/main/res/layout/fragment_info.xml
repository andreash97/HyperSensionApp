            android:layout_height="505dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:translationY="80dp" android:translationX="5dp"
            android:scrollbarSize="10dp">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical" android:scrollbarDefaultDelayBeforeFade="400"
                      android:scrollbarSize="4dp" android:verticalScrollbarPosition="defaultPosition"
